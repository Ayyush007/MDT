<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-37dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="410dp"
            android:layout_height="490dp"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/top_intro_background"
            tools:layout_editor_absoluteY="-70dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/Hello"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Welcome Back"
            android:textColor="@color/white"
            android:textSize="24sp"
            android:textStyle="italic"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="34dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/Anjali"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Have a great day!"
            android:textColor="#ffffff"
            android:textSize="24sp"
            android:textStyle="italic"
            tools:layout_editor_absoluteX="62dp"
            tools:layout_editor_absoluteY="82dp"
            tools:ignore="MissingConstraints" />

        <LinearLayout
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp"
            android:elevation="4dp"
            android:orientation="horizontal"
            android:rotation="0"
            app:layout_constraintBottom_toBottomOf="@+id/Hello"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Anjali"
            app:layout_constraintVertical_bias="0.518">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="67dp"
                android:layout_height="119dp"
                app:srcCompat="@drawable/bottom_btn2" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="140dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/mobileinfo"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="399dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/Mobile_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mobile Info"
            android:onClick="openMainActivity"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="529dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="135dp"
            android:layout_height="112dp"
            app:srcCompat="@drawable/sensors"
            tools:layout_editor_absoluteX="244dp"
            tools:layout_editor_absoluteY="399dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sensor Test"
            tools:layout_editor_absoluteX="247dp"
            tools:layout_editor_absoluteY="529dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="142dp"
            android:layout_height="114dp"
            app:srcCompat="@drawable/storage"
            tools:layout_editor_absoluteX="134dp"
            tools:layout_editor_absoluteY="603dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data Storage and Memory"
            android:textSize="19sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="82dp"
            tools:layout_editor_absoluteY="743dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/btn_3"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="186dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/btn_2"
            tools:layout_editor_absoluteX="353dp"
            tools:layout_editor_absoluteY="247dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/btn_4"
            tools:layout_editor_absoluteX="268dp"
            tools:layout_editor_absoluteY="186dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="91dp"
            android:layout_height="41dp"
            app:srcCompat="@drawable/bottom_btn5"
            tools:layout_editor_absoluteX="127dp"
            tools:layout_editor_absoluteY="155dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Get your "
            android:textColor="#03A9F4"
            android:textSize="34sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="292dp"
            tools:ignore="DuplicateIds,MissingConstraints" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif-light"
            android:text="mobile info in just one click... "
            android:textColor="#03A9F4"
            android:textSize="24sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="32dp"
            tools:layout_editor_absoluteY="348dp"
            tools:ignore="DuplicateIds,MissingConstraints" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>